var ZYStorage={sessionSet:function(a,b){sessionStorage.setItem(a,JSON.stringify(b))},sessionGet:function(a){var b=sessionStorage.getItem(a);return JSON.parse(b)},sessionRemove:function(a){var b=sessionStorage.removeItem(a);return JSON.parse(b)},localSet:function(a,b){localStorage.setItem(a,JSON.stringify(b))},localGet:function(a){var b=localStorage.getItem(a);return JSON.parse(b)}};