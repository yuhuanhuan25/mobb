$(".dj").mouseover(function(){$(".cls-1").css("fill","#59c7f9")}).mouseout(function(){$(".cls-1").css("fill","#76838f")});var showkerid=$(".dj").attr("showkerid");$.ajax({type:"post",url:sUrl+"/shop/showker/checkShowker",data:{showkerId:showkerid},success:function(a){if(a===false){$(".dj").remove();$(".showkerimg").attr("href","#")}}});if($(".dj").attr("showkerid")==="xxxx"){$(".dj").remove();$(".showkerimg").attr("href","#")}$(".imglink").click(function(a){if($(a.target).parent().attr("href").indexOf("recId")===-1){var b=$(a.target).parent().attr("href")+"?recId="+$(a.target).parent().attr("recommendid");$(a.target).parent().attr("href",b)}});$(".xq").click(function(a){if($(a.target).attr("href").indexOf("recId")===-1){var b=$(a.target).attr("href")+"?recId="+$(a.target).attr("recommendid");$(a.target).attr("href",b)}});function GetQueryString(a){var b=new RegExp("(^|&)"+a+"=([^&]*)(&|$)");var c=window.location.search.substr(1).match(b);if(c!=null){return unescape(c[2])}return null}var myurl=GetQueryString("recId");if(myurl!=null&&myurl.toString().length>1){$(".e_name").attr("recommendid",myurl)}var sc=localStorage.getItem("saw-collection");if(sc){$(".collected").css("display","none")}else{setTimeout(function(){$(".el i").animate({width:"150px"},2000)},1000);setTimeout(function(){$(".collected").css("display","none")},5000)}localStorage.setItem("saw-collection","true");var tiphtml="";tiphtml+='<div style="width:92px;height:36px;font-size:12px;position:absolute;color:#fff;background: #000; display:none;" id="littletip"><p style="padding:10px;">点击预览模板</p><em style="background: #000;position: absolute;left: 22px;bottom: -6px;width: 12px;height: 12px;transform: rotate(45deg);"></em></div>';$("body").append(tiphtml);function tooltip(a){var b=null;$(a).mousemove(function(d){$("#littletip").css("display","none");clearTimeout(b);var f=parseInt(d.pageX-30)+"px";var c=parseInt(d.pageY-50)+"px";$("#littletip").css("left",f);$("#littletip").css("top",c);b=setTimeout(function(){$("#littletip").css("display","block")},2000)}).mouseout(function(){clearTimeout(b);$("#littletip").css("display","none")})}tooltip(".imglink");tooltip(".xq");function insshow(){$(".ins_box").css("display","block")}function inshide(){$(".ins_box").css("display","none")}$(".copylink").click(function(){var a=$(".copylink").offset().left-$(".el").offset().left+88;$(".linkhide").css("display","inline-block").css("left",a);setTimeout(function(){$(".linkhide").css("display","none")},1000)});$(".iframe_panel").css("border-right","0");$(".ewm").mouseover(function(){$(".ewm-up-clothes").css("display","block");$(".ewm-up").css("right","-319px");$(".ewm-up").css("display","block");$(".ewm-up").animate({right:"0"})}).mouseout(function(){$(".ewm-up-clothes").css("display","none")});var spanwidth=0;var allwidth=$(".pre_del").width();var alllength=$(".bqtag").children().length;for(var i=0;i<alllength;i++){spanwidth=spanwidth+$(".bqtag").children().eq(i).width()+32;if(spanwidth>=allwidth){$(".bqtag").children().eq(i-1).nextAll().remove();break}}$(".copylink").click(function(){copyText($(".copylink").attr("h5src"))});function copyText(d){var c=document.createElement("textarea");c.style.position="absolute";c.style.left="-9999px";c.style.bottom="0";document.body.appendChild(c);c.textContent=d;c.focus();c.setSelectionRange(0,c.value.length);var a;try{a=document.execCommand("copy")}catch(b){a=false}document.body.removeChild(c);return a}var qrcode=new QRCode("qrcode",{text:$(".copylink").attr("h5src"),width:180,height:180,colorDark:"#000000",colorLight:"#ffffff",correctLevel:QRCode.CorrectLevel.H});qrcode.makeCode(qrcode.text);